<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<ScrollPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.goiching.iching.plugin.plumblossom.longterm.controller.Result"
            fitToWidth="True" fitToHeight="True">

  <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">

      <Tab fx:id="_tabClassic">
          <content>
              <fx:include fx:id="_classic" source="/longterm/fxml/Classic.fxml"/>
          </content>
      </Tab>

      <Tab fx:id="_tabWithoutLine">
          <content>
              <fx:include fx:id="_withoutLine" source="/longterm/fxml/WithoutLine.fxml"/>
          </content>
      </Tab>

      <Tab fx:id="_tabMovingLine">
          <fx:include fx:id="_movingLine" source="/longterm/fxml/MovingLine.fxml"/>
      </Tab>

  </TabPane>

</ScrollPane>
